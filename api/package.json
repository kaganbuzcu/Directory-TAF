{
  "name": "cub-node-js",
  "version": "1.0.0",
  "description": "cub directory app web service",
  "engines": {
    "node": "12.x"
  },
  "main": "index.js",
  "scripts": {
    "build": "babel src -d dist",
    "clean": "rimraf dist",
    "dev:build": "npm-run-all --parallel clean start",
    "dev": "nodemon --watch src --exec npm run dev:build",
    "start": "nodemon --exec babel-node index.js"
  },
  "dependencies": {
    "body-parser": "1.18.3",
    "colorthief": "^2.3.0",
    "cors": "2.8.4",
    "dotenv": "6.0.0",
    "express": "4.16.3",
    "morgan": "1.9.1",
    "pg": "^8.5.1",
    "rimraf": "2.6.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "4.1.2",
    "chai-http": "4.2.0",
    "eslint": "5.6.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.14.0",
    "mocha": "5.2.0",
    "nodemon": "^1.19.4",
    "npm-run-all": "4.1.3"
  }
}
